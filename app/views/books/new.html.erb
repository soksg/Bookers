<h1>New book</h1>
  <%= form_with model: @book, url: "/books", method: :post  do |f| %>
<label>Title</label>
  <%= f.text_field :title %>
<label>Body</label>
  <%= f.text_area %>

  <%= f.submit 'Create Book' %>

  <% end %>


